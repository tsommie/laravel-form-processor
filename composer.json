{
    "name": "ac-developers/laravel-form-processor",
    "description": "An easy way to process multiple form requests from one controller method.",
    "keywords": ["laravel", "lumen", "laravel-form-processor", "form", "Anitche Chisom"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "laravel/framework": "^5.0"
    },
    "authors": [
        {
            "name": "Anitche Chisom",
            "email": "anitchec.dev@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "AcDevelopers\\LaravelFormProcessor\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "AcDevelopers\\LaravelFormProcessor\\LaravelFormProcessorServiceProvider"
            ],
            "aliases": {
                "LaravelFormProcessorFacade": "AcDevelopers\\LaravelFormProcessor\\LaravelFormProcessorFacade"
            }
        }
    }
}
